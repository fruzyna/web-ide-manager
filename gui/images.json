{
    "frc-java": {
        "environment": [
            "PUID=1000", "PGID=1000",
            "TZ=America/Chicago",
            "PASSWORD={{ password }}",
            "SUDO_PASSWORD={{ sudo_password }}"
        ],
        "port": 8443,
        "volume": "/config",
        "image": "mail929/code-server-frc-java",
        "name": "FRC Java",
        "forward-path": true,
        "cpus": 0.5,
        "memory": 2.5,
        "swap": 5.0
    },
    "code-server": {
        "environment": [
            "PUID=1000", "PGID=1000",
            "TZ=America/Chicago",
            "PASSWORD={{ password }}",
            "SUDO_PASSWORD={{ sudo_password }}"
        ],
        "port": 8443,
        "volume": "/config",
        "image": "mail929/code-server-plus",
        "name": "Code Server",
        "forward-path": true,
        "cpus": 0.5,
        "memory": 2.5,
        "swap": 5.0
    },
    "jupyter-ds": {
        "environment": [
            "JUPYTER_ENABLE_LAB=yes",
            "CHOWN_HOME=yes"
        ],
        "port": 8888,
        "volume": "/home/jovyan",
        "image": "jupyter/datascience-notebook",
        "name": "Jupyter",
        "forward-path": false,
        "command": "start.sh jupyter lab --ServerApp.token {{ password }} --ip 0.0.0.0 --no-browser --ServerApp.base_url {{ path }}",
        "cpus": 0.5,
        "memory": 2.5,
        "swap": 5.0
    }
}